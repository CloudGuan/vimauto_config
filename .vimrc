syntax on
syntax enable

set nocompatible
set backspace=indent,eol,start
set syn=cpp
set cindent

set tabstop=4 
set shiftwidth=4
set wrap


set tabstop=4
set softtabstop=4
set shiftwidth=4
set noexpandtab

"encoding
set enc=utf-8
set fenc=utf-8
set termencoding=utf-8
set nocompatible
set tabstop=4        " tab width is 4 spaces
set shiftwidth=4     " indent also with 4 spaces
set fileformat=unix

"color
set t_Co=256
colorscheme gruvbox 
"UI config
set number


